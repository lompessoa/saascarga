datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CargaIncendio {
  id        Int      @id @default(autoincrement())
  codigo    String   @unique
  descricao String
  divisao   String
  carga     Float
  referencia String
}
